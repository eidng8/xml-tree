{
  "name": "g8-xml-edit",
  "version": "0.0.1",
  "description": "A Vue.js component showing and editing XML in tree view.",
  "repository": "git@github.com:eidng8/xml-edit.git",
  "bugs": "git@github.com:eidng8/xml-edit/issues",
  "author": "eidng8",
  "license": "GPL-3.0",
  "main": "./dist/g8-xml-edit.umd.min.js",
  "typings": "./typings/index.d.ts",
  "files": [
    "/dist/*.js",
    "/dist/*.css",
    "/src/index.ts",
    "/src/components",
    "/typings"
  ],
  "keywords": [
    "typescript",
    "vue",
    "vue-component",
    "xml"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --mode production ./src/index.ts && npm run build-sass && tsc",
    "build-dev": "npm run build-app && npm run build-sass",
    "build-sass": "node-sass --output-style compressed src/styles/index.scss > dist/g8-xml-edit.css",
    "build-docs": "vue-docgen && typedoc",
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:cover": "npm run test:unit -- --coverage",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:e2e:visual": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "wba": "cross-env WBA=1 npm run build"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "g8-popup-menu": "0.0.7",
    "g8-vue-tree": "^0.2.4",
    "iconv-lite": "^0.5.1",
    "lodash": "^4.17.17",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "xml-js": "^1.6.11",
    "xmldom-ts": "^0.3.1",
    "xpath-ts": "^1.3.13",
    "xregexp": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.150",
    "@types/nightwatch": "^1.1.3",
    "@types/xregexp": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-nightwatch": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "chromedriver": "latest",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "g8-typedoc-dark": "^0.1.2",
    "husky": "^4.2.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "typedoc": "^0.17.4",
    "typescript": "~3.8.3",
    "vue-docgen-cli": "^4.16.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
